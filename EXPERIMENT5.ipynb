   "source": [
    "Write a Python program to display Monitor Network Statistics (Bytes Sent/Received)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "de457f8b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Bytes Sent: 3.11 MB\n",
      "Bytes Received: 2.28 MB\n"
     ]
    }
   ],
   "source": [
    "import psutil\n",
    "\n",
    "net_io = psutil.net_io_counters()\n",
    "print(f\"Bytes Sent: {net_io.bytes_sent / (1024 ** 2):.2f} MB\")\n",
    "print(f\"Bytes Received: {net_io.bytes_recv / (1024 ** 2):.2f} MB\")"
   ]
  }
 ],
